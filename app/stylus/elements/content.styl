/* ==============
    Styles
   ============== */

body
	font 16px/26px Helvetica, Helvetica Neue, Arial
	
/* ==============
    All: Themes
   ============== */


/* ==============
    WIDE: Menu
   ============== */


header
	position fixed
	top 0
	.header
		.logo
			float left
			padding 15px 0
			img
				width 320px
		nav
			float right
			margin-top 20px
					
			ul
				padding 0
				margin 0
				list-style none
				text-align center
				li
					display inline-block
					a
						display block
						padding 10px 30px
						
						border-radius 10px
						
						text-decoration none
						font-size 1.5em
						color negro
						letter-spacing 1.5px
						
						&:hover
							background verde
							color blanco
							box-shadow 4px 4px 20px 0px rgba(50, 50, 50, 0.4)
							
						

